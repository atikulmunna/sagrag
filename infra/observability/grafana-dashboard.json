{
  "title": "SAG-RAG Backend",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests Total",
      "targets": [
        {
          "expr": "sum(sag_rag_requests_total) by (path)",
          "legendFormat": "{{path}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Latency (ms) Total",
      "targets": [
        {
          "expr": "sum(sag_rag_request_latency_ms_total) by (path)",
          "legendFormat": "{{path}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency Buckets (<=250ms)",
      "targets": [
        {
          "expr": "sum(sag_rag_request_latency_ms_bucket{le=\"250\"}) by (path)",
          "legendFormat": "{{path}}"
        }
      ]
    }
  ],
  "schemaVersion": 38,
  "version": 1
}
